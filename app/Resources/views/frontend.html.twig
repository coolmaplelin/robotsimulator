<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% if app.session.get('meta_title') %}{{ app.session.get('meta_title') }}{{ app.session.set('meta_title','') }}{% else %}Robot Simulator{% endif %}</title>
        <meta name="Description" content="{% if app.session.get('meta_description') %}{{ app.session.get('meta_description') }}{{ app.session.set('meta_description','') }}{% endif %}" />
        
        <link rel="canonical" href="{% if app.session.get('canonical') %}{{ app.session.get('canonical') }}{{ app.session.set('canonical','') }}{% else %}{{ app.request.uri }}{% endif %}" />
                
        {% block stylesheets %}
            <link rel="stylesheet" href="{{ asset('bundles/mapescms/css/bootstrap.min.css') }}" />
			<link rel="stylesheet" href="{{ asset('bundles/mapescms/font-awesome/css/font-awesome.min.css') }}" />
			<link rel="stylesheet" href="{{ asset('bundles/mapescms/css/style.css') }}" />
			<link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Montserrat:400,700" >
			<link rel="stylesheet" href='https://fonts.googleapis.com/css?family=Kaushan+Script' rel='stylesheet'>
			<link rel="stylesheet" href='https://fonts.googleapis.com/css?family=Droid+Serif:400,700,400italic,700italic' rel='stylesheet'>
			<link rel="stylesheet" href='https://fonts.googleapis.com/css?family=Roboto+Slab:400,100,300,700' rel='stylesheet' >
			
        {% endblock %}

        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
		<link rel="shortcut icon" type="image/x-icon" href="{{ asset('robot.png') }}" >
        
    </head>
	<body id="page-top" class="index">


			{% block content %}{% endblock %}

			
			{% block javascripts %}
				{# jQuery #}
				<script src="{{ asset('bundles/mapescms/js/jquery.js') }}"></script>

				{# Bootstrap Core JavaScript #}
				<script src="{{ asset('bundles/mapescms/js/bootstrap.min.js') }}"></script>
			
				<script src="{{ asset('bundles/mapescms/js/robot.js') }}"></script>
			{% endblock %}
			
	</body>
</html>
